</div>

<div class="m-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="m-portlet">
                <div class="m-portlet__head">
                    <div class="m-portlet__head-caption">
                        <div class="m-portlet__head-title">
                            <span class="m-portlet__head-icon">
                                <i class="la la-cubes"></i>
                            </span>
                            <h3 class="m-portlet__head-text">
                                パラメータ
                            </h3>
                        </div>
                    </div>
                </div>
                <!--begin::Form-->
                <div class="m-portlet__body">
                    <div class="m-section">
                        <div class="m-section__content">
                            <div class="m-separator m-separator--dashed"></div>
                            <div class="row  m--padding-top-20 m--padding-bottom-20">
                                @foreach (var abnormalTrajectoryEvaluation in Model.AbnormalTrajectoryEvaluationCollection)
                                {
                                    <div class="col-lg-4 m--margin-top-20">
                                        <div class="row">
                                            <div class="col-lg-12 offset-lg-1 m--font-bold">
                                                <h6 class="m--font-boldest">ThACC: @abnormalTrajectoryEvaluation.ThACC , ThFOC: @abnormalTrajectoryEvaluation.ThFOC</h6>
                                            </div>
                                        </div>
                                        <div class="row m--margin-top-15">
                                            <div class="col-lg-5 offset-lg-1 m--font-bold">
                                                適合率
                                            </div>
                                            <div class="col-lg-6">
                                                @abnormalTrajectoryEvaluation.Precision
                                            </div>
                                        </div>
                                        <div class="row m--margin-top-15">
                                            <div class="col-lg-5 offset-lg-1 m--font-bold">
                                                再現率
                                            </div>
                                            <div class="col-lg-6">
                                                @abnormalTrajectoryEvaluation.Recall
                                            </div>

                                        </div>
                                        <div class="row m--margin-top-15">
                                            <div class="col-lg-5 offset-lg-1 m--font-bold">
                                                F値
                                            </div>
                                            <div class="col-lg-6">
                                                @abnormalTrajectoryEvaluation.FMeasure
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
                <!--end::Form-->
            </div>
        </div>
    </div>

</div>



